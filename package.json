{
  "name": "example-joker",
  "version": "0.1.3",
  "description": "An example application that tells bad jokes",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "node src/js/joker.test.js",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fermain/joker.git"
  },
  "keywords": [
    "joke"
  ],
  "author": "Oliver Dipple <oliver.dipple@noroff.no>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Fermain/joker/issues"
  },
  "homepage": "https://github.com/Fermain/joker#readme",
  "devDependencies": {
    "eslint": "^8.25.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "give-me-a-joke": "^0.5.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
